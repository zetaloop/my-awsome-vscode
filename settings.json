{
    "animations.Install-Method": "Custom UI Style",
    "python.analysis.autoFormatStrings": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.aiCodeActions": {
        "convertFormatString": true,
        "implementAbstractClasses": true,
        "generateSymbol": true,
        "generateDocstring": true
    },
    "code-runner.saveFileBeforeRun": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.ignoreSelection": true,
    "files.autoSave": "onFocusChange",
    "editor.fontFamily": "'Maple Mono Normal NF CN Light'", //'JetBrains Mono', 'SolsticeX',
    // Thin < ExtraLight < Light < Regular < Medium < SemiBold < Bold < ExtraBold
    "editor.fontLigatures": true,
    "animations.CursorAnimation": true,
    "animations.Smooth-Mode": false,
    "animations.CursorAnimationOptions": {
        "Color": "#8888",
        "CursorStyle": "line",
        "TrailLength": 8
    },
    "animations.Durations": {
        "Command-Palette": 100,
        "Tabs": 200,
        "Active": 200,
        "Scrolling": 200,
        "Smooth-Mode": 200,
        "Focus-Dimming": 200
    },
    "animations.Scrolling": "None",
    "security.workspace.trust.enabled": false,
    "markdown.extension.completion.enabled": true,
    "markdown.extension.theming.decoration.renderHardLineBreak": true,
    "markdown.extension.theming.decoration.renderParagraph": true,
    "markdown.extension.toc.orderedList": true,
    "github.copilot.enable": {
        "plaintext": true,
        "markdown": true
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "security.allowedUNCHosts": ["wsl.localhost"],
    "redhat.telemetry.enabled": false,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "prettier.tabWidth": 2,
        "editor.tabSize": 2,
        "editor.detectIndentation": false
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.detectIndentation": false,
        "editor.tabSize": 2,
        "prettier.tabWidth": 2
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[nginx]": {
        "editor.defaultFormatter": "ahmadalli.vscode-nginx-conf"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "markdown.preview.breaks": true,
    "explorer.confirmDelete": true,
    "editor.mouseWheelScrollSensitivity": 4,
    "workbench.list.mouseWheelScrollSensitivity": 4,
    "terminal.integrated.mouseWheelScrollSensitivity": 4,
    "editor.fastScrollSensitivity": 3,
    "workbench.list.fastScrollSensitivity": 3,
    "terminal.integrated.fastScrollSensitivity": 3,
    "editor.unicodeHighlight.allowedLocales": {
        "zh-hans": true,
        "zh-hant": true
    },
    "terminal.integrated.env.windows": {},
    "python.analysis.typeCheckingMode": "standard",
    "code-runner.runInTerminal": true,
    "cmake.options.statusBarVisibility": "compact",
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "cmake.showOptionsMovedNotification": false,
    "prettier.experimentalTernaries": true,
    "prettier.tabWidth": 4,
    "github.copilot.chat.search.semanticTextResults": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "python.analysis.supportDocstringTemplate": true,
    "python.terminal.shellIntegration.enabled": true,
    "githubPullRequests.pullBranch": "never",
    "autoit.aiPath": "C:\\Applications\\AutoIt\\AutoIt3\\AutoIt3.exe",
    "autoit.UDFCreator": "Zetaloop",
    "github.copilot.advanced": {
        "authProvider": "github"
    },
    "editor.largeFileOptimizations": false,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.formatOnSave": true,
    "github.copilot.chat.editor.temporalContext.enabled": true,
    "github.copilot.chat.languageContext.typescript.enabled": true,
    "editor.accessibilitySupport": "off",
    "notebook.inlineValues": "on",
    "terminal.integrated.fontLigatures.enabled": true,
    "terminal.integrated.enableVisualBell": true,
    "terminal.integrated.tabs.title": "${process}${progress}",
    "terminal.integrated.stickyScroll.enabled": true,
    "terminal.integrated.suggest.enabled": true,
    "vscode_custom_css.statusbar": false,
    "latex-workshop.latex.tools": [
        {
            "name": "tectonic",
            "command": "tectonic",
            "args": ["--synctex", "--keep-logs", "--print", "%DOC%.tex"],
